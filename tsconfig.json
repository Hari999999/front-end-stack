{
  "compilerOptions": {
    "allowJs": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "noUnusedLocals": true,
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
