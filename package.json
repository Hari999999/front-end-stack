{
  "name": "base-atomic-design",
  "version": "1.0.0",
  "description": "Bootstrap to get your next project off the ground, based on Atomic Design thinking.",
  "license": "UNLICENSED",
  "private": true,
  "author": "Te Riu Warren <te@pixelfusion.co.nz>",
  "contributors": [
    "Tia Aoake <tia@pixelfusion.co.nz>",
    "Damian Peterson <dp@pixelfusion.co.nz>"
  ],
  "main": "public/index.html",
  "keywords": [],
  "scripts": {
    "build": "webpack",
    "postinstall": "npm run build",
    "preinstall": "cp ./__git__/hooks/* ./.git/hooks && chmod +x ./.git/hooks/pre-*",
    "lint-js": "eslint \"resources/assets/js/**/*.{js,jsx}\" --quiet --fix",
    "lint-scss": "stylelint resources/assets/sass/**/*.scss --syntax scss",
    "lint": "npm run lint-scss && npm run lint-js",
    "unit": "jest"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pixelfusion/base-atomic-design.git"
  },
  "homepage": "https://github.com/pixelfusion/base-atomic-design#readme",
  "devDependencies": {
    "assets-webpack-plugin": "^3.3.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-jest": "12.0.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-react": "^5.0.1",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "imports-loader": "0.6.5",
    "jest-cli": "^12.0.1",
    "json-loader": "^0.5.4",
    "path": "^0.12.7",
    "postcss": "^5.0.20",
    "postcss-cssnext": "^2.5.2",
    "postcss-discard-duplicates": "^2.0.1",
    "postcss-easy-import": "^1.0.1",
    "postcss-inline-comment": "^2.0.0",
    "postcss-loader": "^0.9.1",
    "postcss-mixins": "^4.0.1",
    "postcss-remify": "^1.0.1",
    "postcss-scss": "^0.1.7",
    "postcss-sorting": "^1.3.1",
    "postcss-strip-inline-comments": "^0.1.3",
    "precss": "^1.4.0",
    "react-addons-test-utils": "0.14.7",
    "style-loader": "^0.13.0",
    "stylelint": "^6.2.2",
    "stylelint-loader": "^5.0.1",
    "webpack": "^1.13.00",
    "webpack-dev-server": "^1.14.1",
    "webpack-livereload-plugin": "^0.8.1"
  },
  "dependencies": {
    "normalize.css": "^4.0.0",
    "svg4everybody": "^2.0.3"
  }
}
