{
  "name": "front-end-stack",
  "version": "2.0.0",
  "description": "Front End bootstrap to get your next project off the ground, based on Atomic Design thinking.",
  "license": "MIT",
  "private": true,
  "repository": "pixelfusion/front-end-stack",
  "homepage": "https://github.com/pixelfusion/front-end-stack#readme",
  "author": "Tia Aoake <tia@pixelfusion.co.nz>",
  "contributors": [
    "Josh Post <josh@pixelfusion.co.nz>",
    "Anton  Vasetenekov<anton@pixelfusion.co.nz>",
    "Damian Redpath <damian@pixelfusion.co.nz>"
  ],
  "bugs": {
    "url": "https://github.com/pixelfusion/front-end-stack/issues"
  },
  "engines": {
    "node": ">=7"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:server": "webpack -p --config webpack.server.config.js",
    "build:client": "webpack -p --config webpack.client.config.js && sw-precache --config=sw-precache-config.js",
    "start": "NODE_ENV=development nodemon src/server/server.tsx --ignore src/client --ignore public/ --exec 'ts-node'",
    "lint": "npm run lint-css",
    "lint-css": "stylelint \"src/sass/**/*.{css,scss}\" --syntax scss",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "test": "echo 'No tests available'",
    "icon:compress": "svgo -f static/icons --pretty",
    "icon:compile": "spritesh -i ./static/icons -o ./static/icons/_icon.svg"
  },
  "jest": {
    "testRegex": "(/tests/unit/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "transform": {
      "^.+\\.tsx?|jsx?$": "ts-jest"
    },
    "coverageDirectory": "tests/coverage",
    "collectCoverage": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ]
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "spritesh": "^1.2.1",
    "stylelint": "^9.5.0",
    "stylelint-order": "^1.0.0",
    "svgo": "^1.0.5"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/svg4everybody": "^2.1.0",
    "@types/webpack": "^4.4.10",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.4",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "normalize.css": "^8.0.0",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-remify": "^1.0.1",
    "postcss-simple-vars": "^4.1.0",
    "postcss-strip-inline-comments": "^0.1.5",
    "precss": "^3.1.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "style-loader": "^0.22.1",
    "svg4everybody": "^2.0.3",
    "sw-precache": "^5.0.0",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-serve": "^2.0.2",
    "whatwg-fetch": "^2.0.1",
    "winston": "^3.0.0"
  }
}
